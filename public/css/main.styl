@import url(https://fonts.googleapis.com/css?family=Voltaire);
@import url(https://fonts.googleapis.com/css?family=Open+Sans);
*
	border 0
	margin 0
	padding 0
html
	height 100%
body
	font-family 'Open Sans', sans-serif
	font-size 16px
	height 100%
footer
	color #FFF
	font-size .9em
	font-weight 100
	line-height 10px
	a
		color #DDD
	a:hover
		color #FFF
nav
	margin 0
	padding 0
	ul
		margin 1.5em 0 0 0
		li
			font-family 'Voltaire', sans-serif
			font-size 1.3em
			font-weight 100
			height auto
			line-height 45px
			list-style none
			a
				color #FFF
			a:hover
				color #FFF
			a:active
				color #FFF
.Card
	background rgba(0,0,0,0.7)
	height 100%
	position fixed
	text-align center
	&-logo
		margin 3em 0 0 0
.Col
	height auto
	padding 1.7em 0 1.7em 0
	a
		background #67BC6E
		border-radius 4px
		border-bottom solid 5px green
		-webkit-border-radius 4px
		-moz-border-radius 4px
		-o-border-radius 4px
		-ms-border-radius 4px
		color #FFF
		display block
		font-size 1.1em
		font-weight 100
		text-align center
		-webkit-border-radius 4px
		-moz-border-radius 4px
		-o-border-radius 4px
		-ms-border-radius 4px
		padding .5em 2em .5em 2em
		margin 1.5em 0 0 0
		text-transform uppercase
	a:hover
		text-decoration none
		background green
		border-bottom solid 5px #67BC6E
	&-descripcion
		color grey
		font-size .8em
		margin 0 auto
		max-width 85%
		text-align center
		width 100%
	&-titulo
		color #67BC6E
		font-family 'Voltaire', sans-serif
		font-size 1.3em
		margin .5em 0 .2em 0		
.Columnas
	height auto
	margin-top 1em
	text-align center
	&-titulo
		color #67BC6E
		font-family 'Voltaire', sans-serif
		font-weight 100
	&-tituloMarcas
		color #67BC6E
		font-family 'Voltaire', sans-serif
		font-weight 100
		margin-top 2em
.Contenedor-datos
	background #FFF
	border-radius 3px
	font-size .8em
	float right
	-webkit-border-radius 3px
	-moz-webkit-border-radius 3px
	-o-webkit-border-radius 3px
	-ms-webkit-border-radius 3px
	margin 1.4em 0 0 0
	opacity .9
	padding-bottom .1em
	padding-top .8em
	padding-left .7em
	padding-right .7em
	text-align center
	max-width 500px
	a
		color #333
.Columnas-descripcion
	font-family 'Voltaire', sans-serif
	margin 2em 0 0 0
.Contenedor-logo
	text-align center
.Contenedor-texto
	color grey
	font-family 'Voltaire', sans-serif
	font-size 1.5em
	font-weight 100
	line-height 35px
	margin 1em 0 0 0
	text-align center
.Datos-pie
	background #000
	color #FFF
	font-size .8em
	padding 1.5em 0 .5em 0
	a
		color #FFF
.Def-seccion
	color grey
	font-family 'Voltaire', sans-serif
	font-size 1.1em
	font-weight 100
	text-align right
	margin 2em 0 0 0
	span
		color #DDD
.Descripcion-imagen
	margin 3em 0 3em 0 
	text-align center
.Descripcion-texto
	margin 3em 0 0 0 
	text-align left
	a
		background #67bc6e
		border-bottom solid 5px green
		border-radius 4px
		color #FFF
		-webkit-border-radius 4px
		-moz-border-radius 4px
		-o-border-radius 4px
		-ms-border-radius 4px
		padding 1em 3.5em .5em 3.5em
		text-decoration none
	a:hover
		background green
		border-bottom solid 5px #67bc6e
		border-radius 4px
		color #FFF
		-webkit-border-radius 4px
		-moz-border-radius 4px
		-o-border-radius 4px
		-ms-border-radius 4px
		padding 1em 3.5em .5em 3.5em
		text-decoration none
.Descripciones
	margin-bottom 3em
	img
		height auto
		float left
		max-width 180px
		margin-bottom -5em
		margin-right 4em
		padding 0 0 1em 0
		width 100%
.Descripcion-titulo
	color #67bc6e
	font-family 'Voltaire', sans-serif
	font-size 2em
.Descripcion-subtitulo
	font-size 1.2em
	margin -.2em 0 0 0
.Header-interno
	background-image url('../../public/images/header-interno.jpg')
	background-repeat no-repeat
	background-position center
	background-size cover
	height 82px
.Mapa
	border-top solid 1px #DDD
	color grey
	margin 1.5em 0 0 0
	padding 1.5em 0 0 0
.Mas-descripcion
	border-top solid 1px #DDD
	color grey
	margin-bottom 2em
	margin-top -1em
	h2
		color #67bc6e
		font-family 'Voltaire', sans-serif
		font-size 1.3em
	p
		color grey
		font-size .9em
.Menu-pagina
	ul
		margin 1.7em 0 0 0
		li
			display inline-block
			font-family 'Voltaire', sans-serif
			font-size 1.1em
			font-weight 100
			list-style none
			padding 0 .5em 0 .5em
			a
				color #FFF
			a:hover
				color #67bc6e
				text-decoration none
.Menu-paginas
	background #000
	height 70px
	padding-top 0
	padding-left 2.5em
	img
		margin -.8em 0 0 0
.Nofind
	background #DDD
	border-radius 4px
	color grey
	font-size 1em
	margin 0 .5em 0 .5em
	padding .5em .7em .5em .7em
.Page
	background-image url('../../public/images/bg-elbazar.jpg')
	background-repeat no-repeat
	background-position center
	background-size cover
	height 100%
.Pie
	margin 3em 0 0 0
	text-align center
	a
		color grey
.Pie-interno
	background-image url('../../public/images/header-interno.jpg')
	background-repeat no-repeat
	background-position center
	background-size cover
	height 82px
.Prefooter
	background pink
	font-size .8em
	color red
	margin-top 10em
	ul
		text-align center
		li
			display inline-block
			list-style none
			padding .5em 1em 0 0
.Productos-categoria
	border-bottom solid 1px #DDD
	border-top solid 1px #DDD
	ul
		font-size 1em
		li
			font-family 'Voltaire', sans-serif
			display inline-block
			padding .8em 1em 0 0
			span
				color #DDD
			a
				color grey
.Redes
	height auto
	ul
		li
			display inline-block
			padding 2em .7em 0 .7em
			a:hover
				opacity 0.5
.Redes-paginas
	ul
		float right
		margin 1.9em 0 0 0
		li
			display inline-block
			padding 0 .8em 0 .8em
		a:hover
			opacity 0.7
.Redes-finales
	margin-top -.9em
	padding 0
	ul
		margin 0
		padding 0
		li
			display inline-block
			padding 0 .5em 0 .5em
		a:hover
			opacity 0.7
			
.Separato
	background green
	margin 0 0 4em 0
.Tag
	font-size .8em
	a
		color grey
		background none
		border none
		margin 0
		padding 0
	a:hover
		background none
		border none
		color grey
		margin 0
		padding 0
		text-decoration underline
.Texto-contacto
	color grey
	border-right solid 1px #DDD
	font-size .9em
	line-height 28px
	margin 0 0 2.5em 0
	text-align justify
.Texto-queHacemos
	color grey
	font-size .9em
	line-height 28px
	margin 0 0 2.5em 0
	text-align justify
	
.gallery-item
	border 0
	img
		display inline
		height auto
		max-width 130px
		width 100%
	a
		background none
		border 0
	a:hover
		background none
		border none
		
.gallery-item
	border none
	img
		border none
		
.Btn-mercado
	margin 2em 0 0 0
	text-align center
		
